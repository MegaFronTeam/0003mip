mixin sServiseHead(data)
	// start sServiseHead
	+b.SECTION.sServiseHead.section#sServiseHead&attributes(attributes)
		.container.container--sm
			+e.head
				.row
					.col-md-auto
						+e.icon-wrap
							img(src=`img/svg/${data.i}.svg`, alt="").img-svg
							//- +icon(data.i)
					.col-md
						h1!= data.title
						p!= data.t2
			+e.body
				p!= data.p

	// end sServiseHead