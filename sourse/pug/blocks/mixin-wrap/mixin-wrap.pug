include ../../_assets/bemto/bemto
- set_bemto_settings({ modifier: "--" })
include components/menu.pug
include components/icon.pug
include components/breadcrumb.pug
include components/pugination.pug
include components/soc.pug
include components/pic.pug
include components/img.pug
include components/btn.pug

include components/ddgroup.pug

mixin arrow()
	.swiper-button-hand.swiper-button-hand-prev.swiper-button-prev
		+icon('chevron-left')
	.swiper-button-hand.swiper-button-hand-next.swiper-button-next
		+icon('chevron-right')
//- /arrow for swiper-slider

mixin ie11()
	//- modernizer and for ie11
	 
	<!--[if lt IE 11]><p   class="browsehappy container">К сожалению, вы используете устаревший браузер. Пожалуйста, <a href="http://browsehappy.com/" target="_blank">обновите ваш браузер</a>, чтобы улучшить производительность, качество отображаемого материала и повысить безопасность.</p><![endif]-->

mixin addblock()
	+b.addBlock
		+e.btn--minus(type="button")
		input(type="text" value="1")
		+e.btn--plus(type="button")


mixin mainSlider()
	.mainSlider
		.mainSlider__photos
			#mainSliderPhotos.mainSliderPhotos
				- for (var x = 0; x < 4; x++)
					.mainSliderPhotos__slide(style='background-image: url(images/main_slider/1.jpg)')
						.container
							h5
								| Традиции
								br
								| профессионализма,
								br
								| гарантия результата
							.h1(data-text='Дороги') Дороги
							p.big Комплексное проектирование объектов дорожно-транспортной инфраструктуры
							a.mainSliderPhotos__btn.btn.btn--bgWhite(href='#') Подробнее
						.mainSlider__particles(id=`particles-js-${x}`)
			
		.mainSlider__nav.container
			.mainSliderNav__arrow.mainSliderNav__arrow--prev
				svg(xmlns='http://www.w3.org/2000/svg' width='40' height='41' viewBox='0 0 40 41' fill='none')
					path(d='M20.0007 7.30469L22.3507 9.62468L13.0507 18.8224H33.334V22.1131H13.0507L22.3507 31.3108L20.0007 33.6308L6.66732 20.4678L20.0007 7.30469Z' fill='white')
			#mainSliderNav.mainSliderNav
				.mainSliderNav__slide Дороги
				.mainSliderNav__slide Метро
				.mainSliderNav__slide Гражданские объекты
				.mainSliderNav__slide Изыскания
			.mainSliderNav__arrow.mainSliderNav__arrow--next
				svg(xmlns='http://www.w3.org/2000/svg' width='40' height='41' viewBox='0 0 40 41' fill='none')
					path(d='M20.0007 7.30469L22.3507 9.62468L13.0507 18.8224H33.334V22.1131H13.0507L22.3507 31.3108L20.0007 33.6308L6.66732 20.4678L20.0007 7.30469Z' fill='white')

